{
  "name": "tree-transfer",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@9.15.0",
  "scripts": {
    "dev": "pnpm -r --parallel dev",
    "dev:example": "pnpm --filter example dev",
    "dev:component": "pnpm --filter tree-transfer dev",
    "build": "pnpm --filter tree-transfer build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\""
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "^1.18.0",
    "@jeremy-hibiki/eslint-config": "^0.1.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2"
  }
}
